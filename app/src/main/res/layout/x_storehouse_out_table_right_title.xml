<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/merge_sales"
       xmlns:android="http://schemas.android.com/apk/res/android"
       android:layout_width="match_parent"
       android:layout_height="@dimen/dp_35"
       android:orientation="horizontal">

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match" />

    <TextView android:id="@+id/tv_table_title_1"
              style="@style/TextView_Table_Right_40"
        android:textStyle="bold"
              android:text="单位"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match" />

    <TextView android:id="@+id/tv_table_title_2"
        style="@style/TextView_Table_Right_70"
        android:textStyle="bold"
        android:text="移出库位"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match" />

    <TextView android:id="@+id/tv_table_title_3"
              style="@style/TextView_Table_Right_60"
        android:textStyle="bold"
              android:text="移出数量"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match" />

    <TextView android:id="@+id/tv_table_title_4"
              style="@style/TextView_Table_Right_60"
        android:textStyle="bold"
              android:text="待出仓"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match" />

    <TextView android:id="@+id/tv_table_title_5"
              style="@style/TextView_Table_Right_40"
        android:textStyle="bold"
              android:text="操作"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match" />

</LinearLayout>