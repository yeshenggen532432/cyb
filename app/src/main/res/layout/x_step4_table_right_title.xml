<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/merge_sales"
       xmlns:android="http://schemas.android.com/apk/res/android"
       android:layout_width="match_parent"
       android:layout_height="@dimen/dp_35"
       android:orientation="horizontal">

    <include layout="@layout/line1_v_match"/>
    <TextView android:id="@+id/tv_table_title_1"
              style="@style/TextView_Table_Right_60"
        android:textStyle="bold"
              android:text="到货量"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match"/>

    <TextView android:id="@+id/tv_table_title_2"
              style="@style/TextView_Table_Right_60"
        android:textStyle="bold"
              android:text="实销量"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match"/>

    <TextView android:id="@+id/tv_table_title_3"
              style="@style/TextView_Table_Right_60"
        android:textStyle="bold"
              android:text="库存量"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match"/>

    <TextView android:id="@+id/tv_table_title_4"
        style="@style/TextView_Table_Right_60"
        android:textStyle="bold"
        android:text="订单"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match"/>

    <TextView
        android:id="@+id/tv_table_title_5"
        style="@style/TextView_Table_Right_70"
        android:textStyle="bold"
        android:text="销售类型" />
    <include
        android:visibility="visible"
        layout="@layout/line1_v_match"/>

    <TextView android:id="@+id/tv_table_title_6"
        style="@style/TextView_Table_Right_70"
        android:textStyle="bold"
        android:text="新鲜值"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match"/>

    <TextView android:id="@+id/tv_table_title_7"
              style="@style/TextView_Table_Right_60"
        android:textStyle="bold"
              android:text="规格"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match"/>

    <TextView android:id="@+id/tv_table_title_8"
              style="@style/TextView_Table_Right"
        android:textStyle="bold"
              android:text="备注"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match"/>

    <TextView android:id="@+id/tv_table_title_9"
              style="@style/TextView_Table_Right_40"
        android:textStyle="bold"
              android:text="操作"/>

    <include
        android:visibility="visible"
        layout="@layout/line1_v_match"/>



</LinearLayout>